# ARGS: diverge

main {
init:
  zero: int = const 0;
  one: int = const 1;
  two: int = const 2;
  three: int = const 3;
pack:
  a0: int = const 7;
  a1: int = const 11;
  vec0: vector = s2vb a0;
  vec1: vector = s2vb a1;
  vec2: vector = s2v zero one two three;
  vec3: vector = s2v one zero two three;
  p0: pred = vcmp vec2 vec3;
add0:
  (p0) vec4: vector = vadd vec0 vec0;
add1:
  (!p0) vec5: vector = vadd vec1 vec1;
phi:
  vec6: vector = vphi p0 vec4 vec5;
extract:
  a2: int = v2s vec6 zero;
  a3: int = v2s vec6 one;
  a4: int = v2s vec6 two;
  a5: int = v2s vec6 three;
print:
  print a2;
  print a3;
  print a4;
  print a5;
}
